{
  "name": "nextjs-spotify-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/react-icons": "^3.0.0",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.11.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "11.1.2",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^5",
    "xo": "^0.56.0"
  },
  "xo": {
    "rules": {
      "@typescript-eslint/comma-dangle": [
        0,
        "never"
      ],
      "@typescript-eslint/object-curly-spacing": [
        "error",
        "always"
      ],
      "@typescript-eslint/quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "import/extensions": [
        2,
        "never"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "new-cap": [
        "error",
        {
          "capIsNew": false
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "camelCase": true,
            "pascalCase": true
          }
        }
      ]
    }
  }
}
